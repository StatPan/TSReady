{
  "name": "tsready",
  "version": "1.0.6",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "main": "npx tsx ./src/main.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "postinstall": "husky"
  },
  "files": [
    "src",
    "tsconfig.json",
    "package.json",
    ".eslintignore",
    ".eslintrc.json",
    ".prettierrc.json",
    ".vscode",
    ".husky",
    "package-lock.json"
  ],
  "author": "STATPAN",
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "tsx": "^4.11.0",
    "typescript": "^5.1.3"
  }
}
